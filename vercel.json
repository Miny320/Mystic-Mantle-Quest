{
  "version": 2,
  "buildCommand": "echo 'No build command needed for static Unity WebGL build'",
  "outputDirectory": ".",
  "public": true,
  "headers": [
    {
      "source": "/webglbuild/Build/webglbuild.loader.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    },
    {
      "source": "/webglbuild/Build/webglbuild.framework.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    },
    {
      "source": "/webglbuild/Build/webglbuild.wasm",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    },
    {
      "source": "/webglbuild/Build/webglbuild.data",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/octet-stream"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    },
    {
      "source": "/webglbuild/lib/thirdweb-unity-bridge.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    },
    {
      "source": "/webglbuild/TemplateData/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    },
    {
      "source": "/webglbuild/StreamingAssets/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/octet-stream"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/webglbuild/Build/(.*)",
      "destination": "/webglbuild/Build/$1"
    },
    {
      "source": "/webglbuild/lib/(.*)",
      "destination": "/webglbuild/lib/$1"
    },
    {
      "source": "/webglbuild/TemplateData/(.*)",
      "destination": "/webglbuild/TemplateData/$1"
    },
    {
      "source": "/webglbuild/StreamingAssets/(.*)",
      "destination": "/webglbuild/StreamingAssets/$1"
    },
    {
      "source": "/webglbuild/logo.png",
      "destination": "/webglbuild/logo.png"
    },
    {
      "source": "/",
      "destination": "/webglbuild/index.html"
    }
  ]
}
